{
  "name": "l10n-gen",
  "version": "0.0.3",
  "description": "Localization generator for iOS/Android and more",
  "repository": "github:onmap/L10n-Gen",
  "author": "Philip Kramer (philip@onmap.co.il)",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "l10n-gen": "./bin/l10n-gen.js"
  },
  "scripts": {
    "build": "babel src -d dist",
    "prepublishOnly": "yarn build",
    "precommit": "kcd-scripts precommit",
    "test": "jest",
    "test:single": "yarn test --verbose",
    "test:ci": "yarn test --ci --colors --coverage --silent",
    "lint": "kcd-scripts lint",
    "format": "kcd-scripts format"
  },
  "lint-staged": {
    "*.js": [
      "yarn format",
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "axios": "^0.21.1",
    "commander": "^2.13.0",
    "glob": "7.1.7",
    "lodash": "4.17.21",
    "unzip2": "0.2.5",
    "xml2js": "0.4.23",
    "xml2js-xpath": "^0.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-env": "^1.7.0",
    "jest": "22.4.4",
    "kcd-scripts": "0.39.0"
  }
}
